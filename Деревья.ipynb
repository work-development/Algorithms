    "def dfs_post_order(node, T, path =[]) :\n",
    "    if node.left :\n",
    "        path = dfs_post_order(node.left, T+\"l\", path)\n",
    "    if node.right :\n",
    "        path = dfs_post_order(node.right, T+'r', path)\n",
    "    path += [\"{}+{}\".format(node.value, T)]\n",
    "    \n",
    "    return path\n",
    "\n",
    "def solution(s1, s2):\n",
    "    T = ''\n",
    "    TT = ''\n",
    "    return dfs_post_order(s1,T,path=[]) == dfs_post_order(s2,TT,path=[])"
