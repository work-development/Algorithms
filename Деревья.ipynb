    "# Константин\n",
    "\n",
    "def solution(root, result=[]):\n",
    "    main_list = [root]\n",
    "    result.append([root.value])\n",
    "    while main_list:\n",
    "        children = []\n",
    "        for node in main_list:\n",
    "            if node.left:\n",
    "                children.append(node.left)\n",
    "            if node.right:\n",
    "                children.append(node.right)\n",
    "        main_list = []\n",
    "        if children:\n",
    "            result.append([node.value for node in children])\n",
    "            main_list.extend(children)\n",
    "    return result"
   ]
