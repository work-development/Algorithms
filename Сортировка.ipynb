    "#Сортировка выбором (на месте)\n",
    "def selection_sort(l):\n",
    "    for i in range(len(l)-1):\n",
    "        need_replace = False\n",
    "        max_elem = l[i]\n",
    "        for j in range(i, len(l)):\n",
    "            if l[j] > max_elem:\n",
    "                need_replace = True\n",
    "                max_elem = l[j]\n",
    "                max_pos = j\n",
    "        if need_replace:\n",
    "            tmp = l[i]\n",
    "            l[i] = max_elem\n",
    "            l[max_pos] = tmp\n",
    "    return l"
   ]
