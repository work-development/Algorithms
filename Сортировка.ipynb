    "def sortList(head):\n",
    "    if not head:\n",
    "        return None\n",
    "\n",
    "    length = 1\n",
    "    node = head\n",
    "    tail = None\n",
    "    while node.next:\n",
    "        node = node.next\n",
    "        length += 1\n",
    "    tail = node\n",
    "    return merge_sort(head, tail, length)"
